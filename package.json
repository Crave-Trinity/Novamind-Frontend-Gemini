    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "8.54.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "23.0.0",
    "make-error": "^1.3.6",
    "postcss": "^8.5.3",
    "react-refresh": "^0.17.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.2",
