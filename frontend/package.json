{
  "name": "novamind-digital-twin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:legacy": "vite build --legacy",
    "build:relaxed": "tsc --project tsconfig.relaxed.json && vite build --config vite.config.prod.enhanced.ts",
    "build:prod": "NODE_OPTIONS=\"--max-old-space-size=4096\" vite build --config vite.config.prod.enhanced.ts",
    "build:force": "vite build --emptyOutDir",
    "preview": "vite preview",
    "preview:prod": "vite preview --config vite.config.prod.enhanced.ts",
    "analyze:bundle": "npx vite-bundle-visualizer",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "nodemon --watch src --ext js,ts,tsx --exec \"npm run lint\"",
    "ts-fix": "node scripts/fix-theme-context.js && node scripts/apply-typescript-fixes.js",
    "precommit": "npm run lint && npm run type-check",
    "test:e2e": "cypress run",
    "test:a11y": "npx axe-cli http://localhost:4173 --exit",
    "ci": "npm run ts-fix && npm run build:prod && npm run test",
    "error-watch": "node scripts/continuous-error-check.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@react-three/drei": "^9.88.9",
    "@react-three/fiber": "^8.15.9",
    "@react-three/postprocessing": "^2.15.11",
    "@tanstack/react-query": "^5.25.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.2",
    "classnames": "^2.5.1",
    "d3": "^7.9.0",
    "date-fns": "^3.4.0",
    "framer-motion": "^11.0.8",
    "gsap": "^3.12.5",
    "leva": "^0.9.35",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.2",
    "three": "^0.160.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/classnames": "^2.3.0",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/three": "^0.161.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "globals": "^13.24.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "overrides": {
    "lodash.isequal": {
      "version": "4.5.0",
      "resolved": false
    }
  },
  "resolutions": {
    "three": "^0.160.0"
  }
}
